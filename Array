pragma solidity ^0.8.0;
contract MyContract{
    uint[]public arr=[1,2,3,4,5];
    string[]public str=["apple","banan","carot"];
    string[]public myarr;//satate var
    function addValue(string memory _value)public{
        myarr.push(_value);//local var
    }
    function valueCount()public view returns(uint){
        return myarr.length;
    }
    uint256[][]public arr2d=[[1,2,3],[4,5,6],[7,8,9]];
}
